# Supabase PostgreSQL Connection
# Connection pooler (for queries)
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@[YOUR-REGION].pooler.supabase.com:6543/postgres?pgbouncer=true"
# Direct connection (for migrations)
DIRECT_URL="postgresql://postgres:[YOUR-PASSWORD]@[YOUR-REGION].pooler.supabase.com:5432/postgres"

SCRAPER_USERNAME="your-scraper-username"
SCRAPER_PASSWORD="your-scraper-password"
PORT="3000"
NODE_ENV="development"
ENABLE_AUTH="false"

# Security
CRON_SECRET="your-secure-cron-secret"
ADMIN_PASSWORD="your-admin-login-password"

# Notifications
NOTIFICATION_WEBHOOK_URL="https://your-webhook-endpoint.com"
